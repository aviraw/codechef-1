#include <stdio.h>
#include <iostream>
using namespace std;
int main()
{
    printf("Hello World");
    
    int t;
    cin>>t;
    while(t--)
    {
    
        string A ;
        cin>>A;
        int n;
        n=A.length();
        int count=0;
        char  x[n];
        for(int i=0;i<n-2;i++)
        {
            for(int j=2;j<n;j++)
            {
                if((A[i]==A[j])&&(A[i+1]==A[j+1])&&(i!=j) )
                {
                    if((x[i] != A[i]) && x[i+1] != A[i+1])
                    {
                        count++;
                        x[i]=A[i];
                        x[i+1]=A[i+1];
                    //    cout<<A[i]<<A[i+1]<<" ";
                    }
                }
            }
        }
        cout<<n-count-1<<endl;
            
            
            
        
    }
    return 0;
}
