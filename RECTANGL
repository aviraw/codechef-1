#include <stdio.h>
#include <iostream>
using namespace std;
int main()
{

    long int n=0,a,b,c,d;
    cin>>n;
    
    while(n--)
    {
        cin>>a>>b>>c>>d;
        if((a==b) &&(c==d) &&(a!=c))
        cout<<"YES"<<endl;
        else if((a==c) &&(b==d) &&(a!=b))
        cout<<"YES"<<endl;
        else if((a==d) &&(c==b) &&(a!=c))
        cout<<"YES"<<endl;
        else if((a==b) && (b==c) && (c==d))
        cout<<"YES"<<endl;
        
        else
        cout<<"NO"<<endl;
        
        
    }
    return 0;
}
