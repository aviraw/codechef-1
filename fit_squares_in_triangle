
#include <iostream>
using namespace std;

int main()
{
   
    int t=0 , count = 0;
    cin>>t;
    for(int k=0;k<t;k++)
    {
        int b=0;
        cin>>b;
        count=0;
        for(int i=b-1;i>0;i=i-2)
        {
             count = count + (2*i - 1)/4;
        }
        cout<<count<<endl;
    }
    return 0;
}
