#include <iostream>
#include <stdio.h>
#include <string.h>
using namespace std ;

int main()
{
    int n;
    cin>>n;
    while(n--)
    {
        string s;
        cin>>s;
        int count=0;
        for(int i=0 ; i<s.length() ; i=i+2)
        {
            if(s[i] == s[i+1])
            count++;
            
        }
        if(count == 0)
        cout<<"yes"<<endl;
        
        else
        cout<<"no"<<endl;
    }
}
